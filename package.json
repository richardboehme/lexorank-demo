    "@popperjs/core": "^2.7.2",
    "@rails/actioncable": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "4.3.0",
    "bootstrap": "^5.0.0-beta2",
    "bootstrap-icons": "^1.3.0",
